SELECT 
    s.student_id,
    s.frist_name,
    s.last_name,
    
    e.grade
    
FROM Students s
JOIN Enrollments e ON s.student_id = e.student_id;


SELECT
    c.course_id,
    c.course_name,
    c.course_credits,
    e.enrollment_date
FROM courses c
left JOIN enrollments e on c.course_id = e.course_id;
