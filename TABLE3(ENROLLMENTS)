CREATE TABLE Enrollments (
    enrollment_id INTEGER,
    student_id INTEGER,
    course_id INTEGER,
    enrollment_date DATE,
    grade VARCHAR2(2)
);
INSERT INTO Enrollments (enrollment_id, student_id, course_id, enrollment_date, grade)
